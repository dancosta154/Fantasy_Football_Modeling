                    var element = e.sender.element;
                    if ($(window).width() < 1199)
                        $(".k-animation-container").css({ left: 'auto', right: 0 - element.outerWidth() });
                },
                dataSource: {
                    serverFiltering: true,
                    group: { field: "LeagueSortOrder", dir: "asc" },
                    transport: {
                        read: {
                            type: "POST",
                            dataType: "json",
                            data: function () {
                                return {
                                    searchtext: $scope.player_search ? $scope.player_search.value() : ''
                                }
                            },
                            url: '/search'
                        }
                    }
                }
            }
        }

        function GetLeagueName(sort) {
            switch (sort) {
                case 100: return "NFL";
                case 200: return "MLB";
                case 300: return "NBA";
                case 400: return "NHL";
                default: return "Other";
            }
        }

        function getTimeRemaining(endtime) {
            const total = Date.parse(endtime) - Date.parse(new Date());
            const seconds = Math.floor((total / 1000) % 60);
            const minutes = Math.floor((total / 1000 / 60) % 60);
            const hours = Math.floor((total / (1000 * 60 * 60)) % 24);
            const days = Math.floor(total / (1000 * 60 * 60 * 24));

            return {
                total,
                days,
                hours,
                minutes,
                seconds
            };
        }

        function initializeClock(id, endtime) {
            const clock = document.getElementById(id);
            if (clock != null) {
                const daysSpan = clock.querySelector('.days');
                const hoursSpan = clock.querySelector('.hours');
                const minutesSpan = clock.querySelector('.minutes');
                const secondsSpan = clock.querySelector('.seconds');
                const timeinterval = setInterval(updateClock, 1000);

                function updateClock() {
                    const t = getTimeRemaining(endtime);

                    daysSpan.innerHTML = t.days;
                    hoursSpan.innerHTML = t.hours;
                    minutesSpan.innerHTML = t.minutes;
                    secondsSpan.innerHTML = t.seconds;

                    if (t.total <= 0) {
                        clearInterval(timeinterval);
                    }
                }
                updateClock();
            }            
        }


        const deadline = new Date("May 2, 2022 00:00:00");
        var isLogged = false;
        if (Date.now() < deadline && !isLogged) {
            if (window.innerWidth > 767) {
                initializeClock('clockdiv', deadline);
            } else
                initializeClock('clockdiv-mobile', deadline);

        }


        app.controller("MainLayoutController", MainLayoutController);
</script>


