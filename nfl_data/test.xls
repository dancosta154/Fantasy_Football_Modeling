            $rootScope.bootstrap_gte = function (size) {
                switch (size) {
                    case "xxs": return screenSize.is('xxs, xs, sm, md, lg, xl');
                    case "xs": return screenSize.is('xs, sm, md, lg, xl');
                    case "sm": return screenSize.is('sm, md, lg, xl');
