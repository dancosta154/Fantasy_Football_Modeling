    var FantasyStatsController = function ($scope, $http, $timeout, FantasyStatsViewModel, screenSize) {
        var vm = this;

        delete screenSize.rules.xs;
        angular.extend(screenSize.rules, { xs: '(max-width: 767px) and (min-width: 641px)' });
        angular.extend(screenSize.rules, { xxs: '(max-width: 640px)' });

        vm.media = screenSize.get();
        vm.isMobile = screenSize.is('xxs, xs')
        vm.ShowScore = function () {
            var showscore =
                vm.ViewModel.Filters.scope == 3 ||
                vm.ViewModel.Filters.scope == 2;
            return showscore;
        }

        if (vm.isMobile) {
            vm.FiltersHidden = true;
            $("#collapsible-filters").collapse("hide");
        }

        vm.SetDesktopView = function () {
            if (vm.ViewModel.Filters.desktop)
                vm.ViewModel.Filters.desktop = false;
            else
                vm.ViewModel.Filters.desktop = true;
            $scope.UpdateUrl();
            window.location.reload();
        }

        vm.ViewModel = FantasyStatsViewModel;

        vm.GetMobileSortFields = function () {
            var columns = getColumnSet(vm.ViewModel.Filters.position);
            var fields = [];
            $.each(columns, function (index, column) {
                if (column.field)
                    fields.push({ field: column.field, title: column.title, group: '' });
                else if (column.columns) {
                    $.each(column.columns, function (index, nested_column) {
                        if (nested_column.field)
                            fields.push({ field: nested_column.field, title: nested_column.title, group: column.title });
                    });
                }
            });
            return fields;
        }

        vm.MobileSortOptions = {
            dataSource: new kendo.data.DataSource({
                data: vm.GetMobileSortFields(),
                group: { field: "group" }
            }),
            dataTextField: 'title',
            dataValueField: 'field',
            valueTemplate: "# if (group != '') { # <span style='text-transform: uppercase;'>#: group # </span> # } # <span>#: title #</span>",
            select: function () {
                setTimeout(function () {
                    if (vm.MobileSort) {
                        if (!vm.MobileSort.field) {
                            vm.MobileSort = null;
                            $scope.grid.dataSource.sort(vm.GetDefaultSort());
                        }
                        else
                            $scope.grid.dataSource.sort({ field: vm.MobileSort.field, dir: "desc" });
                        $scope.grid.dataSource.read();
                    }
                }, 50);

            },
            optionLabel: { title: "Sort By...", field: null },
            optionLabelTemplate: '<span>Sort By...</span>'
        };

        vm.GetColumns = function () {
            var columns = [];

            if (vm.isMobile) {
                columns = columns.concat(mobileCols);
                var scoring = vm.ViewModel.Filters.scoringsystem ? scoringCols[vm.ViewModel.Filters.scoringsystem] : scoringCols[1];
                columns.push(scoring[1]);
            }
            else {
                columns = columns.concat(leftCols);
                columns = columns.concat(vm.ViewModel.Filters.scope ? scopeCols[vm.ViewModel.Filters.scope] : scopeCols[1]);
                columns = columns.concat(getColumnSet(vm.ViewModel.Filters.position));
                columns = columns.concat(vm.ViewModel.Filters.scoringsystem ? scoringCols[vm.ViewModel.Filters.scoringsystem] : scoringCols[1]);
            }

            if (!vm.ShowPlayerID)
                columns = columns.filter(function (c) { return !(c.field == "PlayerID"); });

            return columns;
        }

        vm.GetDefaultSort = function () {
            var field = "FantasyPoints";
            if (vm.ViewModel.Filters.scoringsystem)
                field = scoringCols[vm.ViewModel.Filters.scoringsystem][1].field;
            return { field: field, dir: "desc" };
        }

        vm.SetRedZone = function () {
            if (vm.ViewModel.Filters.subscope !=  2)
                vm.ViewModel.Filters.redzonescope = null;
            else if (!vm.ViewModel.Filters.redzonescope)
                vm.ViewModel.Filters.redzonescope = 1;
        }

        vm.SelectScope = function (scope) {
            if (scope == 3 && !vm.ViewModel.LiveEnabled) {
                setTimeout(function () {
                    $("li.k-button").removeClass("k-state-focused");
                }, 10);
                return false;
            }

            vm.ViewModel.AutoRefresh = false;
            vm.ViewModel.Filters.scope = scope;
            switch (vm.ViewModel.Filters.scope) {
                case 3:
                    vm.ViewModel.AutoRefresh = true;
                    vm.RebindGrid();
                    break;
                case 4:
                case 1:
                    vm.ViewModel.Filters.season = vm.ViewModel.DefaultTimeframe.Season;
                    setTimeout(function () {
                        vm.ViewModel.Filters.seasontype = vm.ViewModel.DefaultTimeframe.SeasonType;
                        vm.RebindGrid();
                    }, 10);
                    break;
                case 2:
                    vm.ViewModel.Filters.season = vm.ViewModel.DefaultTimeframe.Season;
                    setTimeout(function () {
                        vm.ViewModel.Filters.seasontype = vm.ViewModel.DefaultTimeframe.SeasonType;
                        setTimeout(function () {
                            vm.ViewModel.Filters.startweek = vm.ViewModel.DefaultTimeframe.Week;
                            vm.ViewModel.Filters.endweek = vm.ViewModel.DefaultTimeframe.Week;
                            vm.RebindGrid();
                        }, 10);
                    }, 10);
                    break;
            }
        }

        vm.SeasonSelect = function () {
            setTimeout(function () {
                $scope.ddl_seasontypes.select(null);
                $scope.ddl_seasontypes.select(0);
                vm.ViewModel.Filters.seasontype = $scope.ddl_seasontypes.dataSource._view[0].SeasonType;
                vm.RebindGrid();
            }, 10)
        }

        vm.SeasonTypeSelect = function () {
            vm.RebindGrid();
        }

        vm.WeekSelect = function () {
            setTimeout(function () {
                if (vm.ViewModel.Filters.endweek < vm.ViewModel.Filters.startweek || vm.ViewModel.Filters.endweek == undefined) {
                    vm.ViewModel.Filters.endweek = $scope.vm.ViewModel.Filters.startweek;
                }

                if (vm.ViewModel.Filters.startweek > vm.ViewModel.Filters.endweek || vm.ViewModel.Filters.startweek == undefined) {
                    vm.ViewModel.Filters.startweek = $scope.vm.ViewModel.Filters.endweek;
                }

                vm.RebindGrid();
            }, 50);
        }

        vm.PlayerSearchClear = function () {
            this.$parent.vm.RebindGrid();
        }

        vm.PlayerSearchSelect = function (e) {
            this.$parent.vm.RebindGrid();
        }

        vm.GetDataSource = function () {
            return new kendo.data.DataSource({
                type: "aspnetmvc-ajax",
                serverSorting: true,
                serverPaging: true,
                transport: {
                    read: {
                        url: "/NFL_FantasyStats/FantasyStats_Read",
                        dataType: "json",
                        data: function () {
                            return {
                                filters: vm.ViewModel.Filters
                            }
                        }
                    }
                },
                schema: {
                    data: "Data",
                    total: "Total",
                    errors: "Errors"
                },
                error: function (e) {
                    if (e.errors.upgrade)
                        $("#window_upgrade_premium").data("kendoWindow").center().open();
                },
                sort: vm.GetDefaultSort()
            });
        };

        vm.GridOptions = {
            dataSource: vm.GetDataSource(),
            autoBind: false,
            groupable: false,
            sortable: true,
            scrollable: true,
            pageable: false,
            noRecords: true,
            dataBound: function () {
                this.gridFixes();
                $scope.LoadFinished();
            },
            columns: vm.GetColumns()
        };

        vm.InitialBind = true;

        vm.RebindGrid = function () {
            $timeout(function () {
                try {
                    $scope.EvaluatePage();
                    var options = vm.GridOptions;
                    options.columns = vm.GetColumns();
                    options.dataSource = vm.GetDataSource();
                    options.dataSource._pageSize = $scope.GetPageSize();
                    $scope.grid.setOptions(options);
                    $scope.grid.gridFixes();
                    $scope.grid.dataSource.read();
                    if (!vm.InitialBind)
                        $scope.UpdateUrl();
                    else
                        vm.InitialBind = false;

                    if ($scope.ddl_mobile_sort) {
                        $scope.ddl_mobile_sort.dataSource.fetch(function () {
                            var data = vm.GetMobileSortFields();
                            $scope.ddl_mobile_sort.dataSource.data(data);
                            vm.MobileSort = null;
                        });
                    }
                }
                catch (ex) {
                    console.log("Error occurred, retry RebindGrid.");
                    console.log(ex);
                    vm.retries = typeof (vm.retries) == 'undefined' ? 3 : vm.retries;
                    if (vm.retries > 0) {
                        vm.RebindGrid();
                        vm.retries--;
                    }
                }
            }, 50)
        }

        screenSize.on('xxs, xs', function (match) {
            if (vm.media != screenSize.get()) {
                vm.media = screenSize.get();
                vm.isMobile = match;
                vm.RebindGrid();

                if (match) {
                    vm.FiltersHidden = true;
                    $("#collapsible-filters").collapse("hide");
                }
                else {
                    vm.FiltersHidden = false;
                    $("#collapsible-filters").collapse("show");
                }
            }
        });

        $(document).ready(function () {
            $scope.vm.RebindGrid();

            setInterval(function () {
                if ($scope.vm.ViewModel.Filters.scope == 3 &&
                    $scope.vm.ViewModel.AutoRefresh == true)
                    $scope.vm.RebindGrid();
            }, 60000);
        })
    }

    var leftCols = [
        { field: "Rank", title: "Rk", width: 40, locked: true },
        { field: "PlayerID", title: "ID", attributes: { "class": "player-id" }, width: 50, locked: true },
        { field: "Name", title: "Name", width: 154, template: kendo.template($("#player_template").html()), headerAttributes: { "class": "align-left" }, attributes: { "class": "align-left" }, locked: true, sortable: true },
        { field: "Team", title: "Team", width: 55, template: "<a href='#: TeamUrlString #'>#: Team #</a>" },
        { field: "Position", title: "Pos", width: 50 }
    ]

    var scopeCols = {
        1: [
            { field: "Played", title: "GMS", width: 45 }
        ],
        4: [
            //{ field: "Played", title: "GMS", width: 45 }
        ],
        2: [
            { field: "Week", title: "WK", width: 38 },
            { field: "Opponent", title: "OPP", width: 42 }
        ],
        3: [
            { field: "Week", title: "WK", width: 38 },
            { field: "Opponent", title: "OPP", width: 42 }
        ]
    };

    var scoringCols = {
        1: [
            { field: "FantasyPointsPerGame", title: "FPTS/G", width: 62 },
            { field: "FantasyPoints", title: "FPTS", width: 60 }
        ],
        2: [
            { field: "FantasyPointsPerGamePPR", title: "PPG", width: 62 },
            { field: "FantasyPointsPPR", title: "FPTS", width: 60 }
        ],
        3: [
            { field: "FantasyPointsPerGameFanDuel", title: "FPTS/G", width: 62 },
            { field: "FantasyPointsFanDuel", title: "FPTS", width: 60 }
        ],
        5: [
            { field: "FantasyPointsPerGameYahoo", title: "FPTS/G", width: 62 },
            { field: "FantasyPointsYahoo", title: "FPTS", width: 60 }
        ],
        4: [
            { field: "FantasyPointsPerGameDraftKings", title: "FPTS/G", width: 62 },
            { field: "FantasyPointsDraftKings", title: "FPTS", width: 60 }
        ],
        7: [
            { field: "FantasyPointsPerGameHalfPointPPR", title: "FPTS/G", width: 62 },
            { field: "FantasyPointsHalfPointPpr", title: "FPTS", width: 60 }
        ],
        8: [
            { field: "FantasyPointsPerGameSixPointPassTd", title: "FPTS/G", width: 62 },
            { field: "FantasyPointsSixPointPassTd", title: "FPTS", width: 60 }
        ],
        6: [
            { field: "FantasyPointsPerGameFantasyDraft", title: "FPTS/G", width: 62 },
            { field: "FantasyPointsFantasyDraft", title: "FPTS", width: 60 }
        ]
